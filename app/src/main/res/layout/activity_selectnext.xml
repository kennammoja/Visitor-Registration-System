<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/to_covid_screening"
        android:layout_width="206dp"
        android:layout_height="95dp"
        android:layout_marginStart="95dp"
        android:layout_marginTop="164dp"
        android:layout_marginEnd="110dp"
        android:layout_marginBottom="56dp"
        android:text="@string/covid_screening"
        app:layout_constraintBottom_toTopOf="@+id/to_log_out"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/to_log_out"
        android:layout_width="191dp"
        android:layout_height="103dp"
        android:layout_marginStart="100dp"
        android:layout_marginBottom="128dp"
        android:text="@string/log_out"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/main_reception"
        android:layout_width="200dp"
        android:layout_height="123dp"
        android:layout_marginStart="93dp"
        android:layout_marginTop="51dp"
        android:layout_marginEnd="118dp"
        android:layout_marginBottom="67dp"
        android:text="MAIN RECEPTION"
        app:layout_constraintBottom_toTopOf="@+id/to_log_out"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/to_covid_screening" />
</androidx.constraintlayout.widget.ConstraintLayout>